<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="com.zoomableview.DepMapView">
        <attr name="mapref" format="reference" />

        <attr name="autoZoomFill" format="boolean" />
        <attr name="autoZoomLevel" format="float" />

        <attr name="maxZoomFill" format="boolean" />
        <attr name="maxZoomLevel" format="float" />

        <attr name="doubletabZoom" format="boolean" />
        
        <!-- 1 slow deceleration, 3 (default), 2 slower deceleration, 4, 5, etc... : faster deceleration -->
        <attr name="flingScale" format="float" />

        <!-- 1: like normal, 0,3 (default), 0: no overscroll -->
        <attr name="overScrollTranslateFactor" format="float" />
    </declare-styleable>

</resources>